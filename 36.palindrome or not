#include <bits/stdc++.h>
using namespace std;

string isPalindrome(string S) {
    string P = S;
    reverse(P.begin(), P.end());
    if (S == P) {
        return "Yes";
    } else {
        return "No";
    }
}

int main() {
    string S = "ABCDCBA"; // Replace with your input string
    cout << isPalindrome(S);
    return 0;
}
